                        error: function (ErrorThrown) {
                            document.getElementById('msg_error').innerHTML = 'Lỗi hệ thống. Vui lòng liên hệ bộ phận kỹ thuật';
                            $('#error').modal('show');
                        }
                    });
                }
            },
            error: function (ErrorThrown) {
                window.location.href = '@Url.Action("Login", "ePOS")';
            }
        })
    }
    function checkCNN(index) {
        $.ajax({
            url: '/ePOS/CheckSession',
            type: 'post',
            success: function (data) {
                if (data.Result == 'ERROR') {
                    window.location.href = '@Url.Action("Login", "ePOS")';
                }
                else {
                    if (index == 0) {
                        if ($('#id_sum').val()) {
                            $.ajax({
                                type: 'get',
                                url: '/Report/_DetailError',
                                success: function (data) {
                                    $('#dynamic-modal').html(data);
                                    $("#dynamic-modal").modal('show');
                                },
                                error: function (ErrorThrown) {
                                    document.getElementById('msg_error').innerHTML = 'Lỗi hệ thống. Vui lòng liên hệ bộ phận kỹ thuật';
                                    $('#error').modal('show');
                                }
                            });
                        }
                    }else if (index == 1) {
                        if ($('#id_sum').val()) {
                            window.location.href = '/Report/ExportCollectionPoint?id=' + encodeURI($('#id_sum').val());
                        }
                    }else if(index == 2) {
                        if ($('#id_detail').val()) {
                            window.location.href = '/Report/ExportDetailPoint?id=' + encodeURI($('#id_detail').val());
                        }
                    } else {
                        if ($('#id_DetailOther').val()) {
                            window.location.href = '/Report/ExportDetailOther?id=' + encodeURI($('#id_DetailOther').val()) + '&fromdate=' + encodeURI($('#id_fromdate').val()) + '&todate=' + encodeURI($('#id_todate').val()) +
                                '&total_trans=' + encodeURI(document.getElementById("total_tran_orther").innerText) + '&total_bill=' + encodeURI(document.getElementById("total_bill_orther").innerText) +
                                '&total_amount=' + encodeURI(document.getElementById("total_amount_orther").innerText);
                        }
                    }
                }
            },
            error: function (ErrorThrown) {
                window.location.href = '@Url.Action("Login", "ePOS")';
            }
        });
    }
    function sourceSum(data) {
        var source =
        {
            id: 'col_1',
            localData: data,
        };
        return source;
    }
    function sourceDetail(data) {
        var source =
        {
            datatype: "array",
            localData: data.Records,
            totalrecords: data.total_bill,
            root: 'Rows',
            beforeprocessing: function (data) {
                source.totalrecords = data[0].col_16;
            }
        };
        return source;
    }

    function sourceDetailWater(data) {
        var source =
        {
            datatype: "array",
            localData: data.Records,
            totalrecords: data.total_tran,
            root: 'Rows',
            beforeprocessing: function (data) {
                source.totalrecords = data[0].col_11;
            }
        };
        return source;
    }

