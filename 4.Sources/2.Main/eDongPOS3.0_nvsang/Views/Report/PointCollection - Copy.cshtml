    function CheckDetail(cellValue) {
        $.ajax({
            url: '/ePOS/CheckSession',
            type: 'post',
            success: function (data) {
                if (data.Result == 'ERROR')
                    window.location.href = '@Url.Action("Login", "ePOS")';
                else {
                    $.ajax({
                        type: 'get',
                        url: '/Report/_DetailHistoryBill',
                        data: {
                            bill: encodeURI($("#DetailContainer").jqxGrid('getrowdata', cellValue).col_8),
                            customer: encodeURI($("#DetailContainer").jqxGrid('getrowdata', cellValue).col_1),
                            amount: encodeURI($("#DetailContainer").jqxGrid('getrowdata', cellValue).col_5)
                        },
                        success: function (data) {
                            $('#dynamic-modal').html(data);
